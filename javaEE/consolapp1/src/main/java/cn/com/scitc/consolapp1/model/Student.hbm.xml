<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.com.scitc.consolapp1.model.Student" table="student" schema="student">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="studentId">
            <column name="studentId" sql-type="char(8)" length="8" not-null="true"/>
        </property>
        <property name="className">
            <column name="className" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="mobile">
            <column name="mobile" sql-type="char(11)" length="11" not-null="true"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>