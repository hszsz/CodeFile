<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.com.scitc.consolapp1.model.Course" table="course" schema="student">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="courseScore">
            <column name="courseScore" sql-type="decimal(2,1)" precision="2" scale="1"/>
        </property>
        <property name="courseTime">
            <column name="courseTime" sql-type="int(11)"/>
        </property>
        <property name="courseType">
            <column name="courseType" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>