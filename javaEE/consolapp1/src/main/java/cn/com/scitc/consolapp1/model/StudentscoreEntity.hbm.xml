<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.com.scitc.consolapp1.model.StudentscoreEntity" table="studentscore" schema="student">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="studentId">
            <column name="studentId" sql-type="char(8)" length="8" not-null="true"/>
        </property>
        <property name="courseId">
            <column name="courseId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="dt">
            <column name="dt" sql-type="date"/>
        </property>
        <property name="score">
            <column name="score" sql-type="decimal(3,1)" precision="3" scale="1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>